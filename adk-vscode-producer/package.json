{
  "name": "adk-telemetry",
  "displayName": "adk-telemetry",
  "description": "A lightweight VS Code agent for ADK-Graph that captures coding activity, file events, and development behavior to generate deep productivity insights.",
  "version": "0.0.2",
  "publisher": "sunilcse2006",
  "icon": "images/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/SunilkumarT56/adk-telemetry"
  },
  "engines": {
    "vscode": "^1.106.1"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "adk-telemetry.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.106.1",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.39.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.1"
  },
  "dependencies": {
    "@types/uuid": "^10.0.0",
    "cross-fetch": "^4.1.0",
    "node-fetch": "^3.3.2",
    "uuid": "^13.0.0"
  }
}
